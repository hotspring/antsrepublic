<p id="notice"><%= notice %></p>

<h1>Listing Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Phone</th>
      <th>Company</th>
      <th>Qq</th>
      <th>Wechat</th>
      <th>Verify img1</th>
      <th>Verify img2</th>
      <th>Verify img3</th>
      <th>Address</th>
      <th>Other</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.phone %></td>
        <td><%= profile.company %></td>
        <td><%= profile.qq %></td>
        <td><%= profile.wechat %></td>
        <td><%= profile.verify_img1 %></td>
        <td><%= profile.verify_img2 %></td>
        <td><%= profile.verify_img3 %></td>
        <td><%= profile.address %></td>
        <td><%= profile.other %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
