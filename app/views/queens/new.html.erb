<%= simple_form_for(@queen) do |f| %>
  <%= f.input :email %>
  <%= f.input :name %>
  <%= f.input :cell %>
  <%= f.input :company %>
  <%= f.input :avatar %>
  <%= f.input :desc %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.button :submit %>
<% end %>
<%= link_to t(:back), :back %>
